                    a.icon.fa-instagram(href='https://www.instagram.com/flydecor_' target='_blank')
                        span.label Instagram
                li
                    a.icon.fa-envelope(href='https://mail.google.com/mail/?view=cm&fs=1&tf=1&to=flydecor65@gmail.com' target='_blank')
